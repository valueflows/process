@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vf: <https://w3id.org/valueflows/> .

# Classes

vf:Agent
  a owl:Class ;
  rdfs:label "Agent" .

vf:Process
  a owl:Class ;
  rdfs:label "Process" .

vf:Transformation
  a owl:Class ;
  rdfs:label "Transformation" ;
  rdfs:subClassOf vf:Process .

vf:Transportation
  a owl:Class ;
  rdfs:label "Transportation" ;
  rdfs:subClassOf vf:Process .

vf:Resource
  a owl:Class ;
  rdfs:label "Resource" .

# Properties

vf:inflow
  a rdf:Property ;
  rdfs:label "inflow" ;
  rdfs:domain vf:Resource ;
  rdfs:range vf:Process .

vf:outflow
  a rdf:Property ;
  rdfs:label "outflow" ;
  rdfs:domain vf:Process ;
  rdfs:range vf:Resource .

vf:consume
  a rdf:Property ;
  rdfs:label "consume" ;
  rdfs:subPropertyOf vf:inflow .

vf:use
  a rdf:Property ;
  rdfs:label "use" ;
  rdfs:subPropertyOf vf:inflow .

vf:create
  a rdf:Property ;
  rdfs:label "create" ;
  rdfs:subPropertyOf vf:outflow .

vf:context
  a owl:ObjectProperty ;
  rdfs:label "context" ;
  rdfs:domain vf:Process ;
  rdfs:range vf:Agent .
